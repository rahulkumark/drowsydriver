{
  "name": "tfjs-webcam-transfer-learning",
  "version": "0.1.0",
  "description": "An app that detects drowsy driver and gives alerts to prevent accidents",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.0",
    "node-static": "^0.7.10",
    "play": "^0.5.0",
    "play-sound": "^1.1.2",
    "vega-embed": "^3.0.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel index.html --no-hmr --open",
    "build": "cross-env NODE_ENV=production parcel build index.html  --no-minify --public-url ./",
    "postinstall": "yarn upgrade --pattern @tensorflow"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "cross-env": "^5.1.6",
    "parcel-bundler": "~1.8.1"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "ios >= 10",
    "android >= 6"
  ]
}
